# Environment variables for Waifu AI Dev
# Copy this file to .env.local and fill in your API keys

# Gemini AI API Key (required for chat functionality)
# Get from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# ElevenLabs API Key (required for text-to-speech)
# Get from: https://elevenlabs.io/speech-synthesis
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# ElevenLabs Voice ID (required for text-to-speech)
# Default voice ID for a female voice, you can change this
VOICE_ID=EXAVITQu4vr4xnSDxMaL

# Node environment
NODE_ENV=development
